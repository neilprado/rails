<p id="notice"><%= notice %></p>

<h1>Tickets</h1>

<table>
  <thead>
    <tr>
      <th>Ganhador</th>
      <th>Telefone</th>
      <th>Filme</th>
      <th>Genero</th>
      <th>Classificacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.ganhador %></td>
        <td><%= ticket.telefone %></td>
        <td><%= ticket.filme %></td>
        <td><%= ticket.genero %></td>
        <td><%= ticket.classificacao %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
