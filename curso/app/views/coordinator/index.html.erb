<h1>Coordinator</h1>
<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Matricula</th>
      <th>Senha</th>
      <th colspan="3"></th>
    </tr>
  </thead>
    <tbody>
      <% @coordinators.each do |coordinator| %>
        <tr>
        <td><%= coordinator.nome %></td>
        <td><%= coordinator.matricula %></td>
        <td><%= coordinator.senha %></td>
        </tr>
      <% end %>  
    </tbody>
  </table>